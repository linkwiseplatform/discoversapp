{
  "rules": {
    ".read": true,
    "admins": {
      ".write": "auth != null && root.child('admins').hasChild(auth.uid)"
    },
    "config": {
      ".write": "auth != null && root.child('admins').hasChild(auth.uid)"
    },
    "userProgress": {
      "$uid": {
        ".write": "auth != null && auth.uid === $uid"
      }
    }
  }
}
